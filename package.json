  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "i": "react-native run-ios --simulator=\"iPhone 15\"",
    "id": "react-native run-ios --device",
