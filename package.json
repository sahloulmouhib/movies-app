    "gradle": "cd android &&./gradlew clean && cd ..",
    "apk": "cd android &&./gradlew assembleRelease",
    "rmnm": "rm -rf node_modules/",
    "stc": "react-native start --reset-cache",
    "ad": "adb devices"
  },
